<!--? $Revision$ $Date$ -->
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <link rel="stylesheet" type="text/css" href="../jmdict.css"/>
  <title>JMdict</title>
</head>
<body>
  <h1>Confirm Submission</h1>
    Please check the entry below carefully.  It it is what you wish 
    to submit, click the submit button.  Otherwise, use your brower's
    <b>Back</b> button to return to the Edit page and make corrections.
    If you have having a problem entering the information you want,
    please send email to 
    <a href="mailto:jmdicthelp@somewhere.org">jmdicthelp@somewhere.org</a>
  <span tal:repeat="e entries">
    <div class="entry">

      <span class="warnings" tal:if="true:chklist">
	<span class="caution">Caution:</span>
	The following existing entries contain kanji or readings
	that exist in your proposed entry.  Before submitting your
	entry, please make sure that it is not a duplicate of, or
	should not be an amendment to, one of these entries.
	<ol>
	  <li tal:repeat="c chklist">
	    <a tal:attributes="href string:entr.pl?svc=${svc}&q=${c/seq}.${c/src}" tal:content="c/seq">seq</a>
	    <span tal:if="c/kanj" tal:content="c/kanj" tal:omit-tag="">kanji</span>
	    <span tal:if="c/kanj">【</span>
	    <span tal:content="c/rdng" tal:omit-tag="">reading</span>
	    <span tal:if="c/kanj">】</span>
	    <span tal:content="c/gloss" tal:omit-tag="">gloss</span>
	    </li>
	  </ol>
	<hr/>
	Your entry:<br/>
	</span>

      <span tal:if="false:e/id" tal:omit-tag="">New Entry
	(<span tal:replace="kwabbr: 'SRC' e/src">src</span>)
	</span>
      <span tal:if="true:e/id" tal:omit-tag="">
        Seq: <span tal:replace="e/seq">seqnum</span>
	(<span tal:replace="kwabbr: 'SRC' e/src">src</span>)
	<span class="status">
	  <span tal:if="true:e/unap" class="pend">
	    <span tal:contents="kwfull: 'STAT' e/stat">status</span> pending
	    </span>
	  <span tal:if="false:e/unap" tal:contents="kwfull: 'STAT' e/stat">status</span>
	  <span tal:if="true:e/dfrm" tal:omit-tag="">
	    <a tal:attributes="href string:entr.pl?svc=${svc}&e=${e/dfrm}" tal:contents="e/dfrm"/>
	    </span>
	  </span>
        <span class="pkid">{id:<span tal:replace="e/id"/>}</span>
        </span>
      <span tal:if="true:e/srcnote">
	<br/>Src Notes: <span tal:replace="e/srcnote" tal:omit-tag="">srcnote</span>
        </span>
      <span tal:if="true:e/notes">
	<br/>Entry Notes: <span tal:replace="e/notes" tal:omit-tag="">srcnote</span>
        </span>

      <br/>
      <span tal:if="e/_kanj" tal:repeat="k e/_kanj" tal:omit-tag="">
	<span tal:condition="not:repeat/start" tal:omit-tag="">； </span>
	<span class="kanj" tal:content="k/txt">kanji text</span>
	<span class="kattrs" tal:if="or:k/_kinf k/_kfreq">
	  [<span class="kinf" tal:if="k/_kinf" tal:content="kwabbrs: 'KINF' ',' k/_kinf">kinf</span>
	   <span tal:if="and: k/_kinf k/_kfreq" tal:replace="string:/">/</span>
	   <span class="kfreq" tal:if="k/_kfreq" tal:content="freqs: ',' k/_kfreq">kfreq</span>]
	  </span>
	</span>

      <br tal:if="e/HAS_RESTR"/>
      <span tal:if="and: e/_kanj e/_rdng" tal:omit-tag="">【</span>
      <span tal:repeat="r e/_rdng">
	<span tal:condition="not:repeat/start" tal:omit-tag="">； </span>
	<span class="rdng" tal:content="r/txt">reading text</span>
	<span class="restr" tal:if="true:/r/_RESTR">(
	  <span tal:if="eq: string:1 r/_RESTR" tal:omit-tag="">no kanji</span>
	  <span tal:if="not:eq: string:1 r/_RESTR" tal:omit-tag="">
	    <span tal:repeat="z r/_RESTR" tal:omit-tag="">
	      <span tal:if="not:repeat/start" tal:omit-tag="">,</span>
	      <span tal:replace="z"/>
	      </span>
	    </span>)
	  </span>
	<span class="rattrs" tal:if="or:r/_rinf r/_rfreq">
	  [<span class="rinf" tal:if="r/_rinf" tal:content="kwabbrs: 'RINF' ',' r/_rinf">rinf</span>
	  <span tal:if="and: r/_rinf r/_rfreq" tal:replace="string:/">/</span>
	  <span class="rfreq" tal:if="r/_rfreq" tal:content="freqs: ',' r/_rfreq">rfreq</span>]
	  </span>
	</span>
      <span tal:if="and: e/_kanj e/_rdng">】</span>

      <table class="senses" border="0">
        <tr class="sense" tal:repeat="s e/_sens">
	  <td valign="top" tal:content="string:${s/sens}.">n.</td>
	  <td colspan="2">
	    <span class="stag" tal:if="s/_STAG">
	      (<span tal:if="s/_STAG" tal:repeat="z s/_STAG" tal:omit-tag="">
		<span tal:if="not:repeat/start" tal:omit-tag="">,</span>
		<span tal:replace="z" tal:omit-tag="">stag-restr</span>
		</span> only)
	      </span>
	    <span class="pos" tal:if="s/_pos">
	      [<span tal:replace="kwabbrs: 'POS' ',' s/_pos">pos</span>]
	      </span>
	    <span class="misc" tal:if="s/_misc">
	      [<span tal:replace="kwabbrs: 'MISC' ',' s/_misc">misc</span>]
	      </span>
	    <span class="fld" tal:if="s/_fld">
	      {<span tal:replace="kwfulls: 'FLD' ',' s/_fld">fields</span>}
	      </span>

	    <span class="dial" tal:condition="s/_dial">, 
	      Dialect: <span tal:replace="kwabbrs: 'DIAL' ',' s/_dial">dial</span>
	      </span>
	    <span class="lang" tal:if="s/_lsrc">, 
	      From: 
	      <span tal:repeat="z s/_lsrc" tal:omit-tag="">
		<span tal:replace="kwabbr: 'LANG' z/lang">(xx)</span
		><span tal:if="or: z/part z/wasei" tal:omit-tag=""
		  >(<span tal:if="z/part" tal:replace="string: p">p</span
		  ><span tal:if="and: z/part z/wasei" tal:replace="string: ,">,</span
		  ><span tal:if="z/wasei" tal:replace="string: w">w</span
		  >)</span
		>:"<span tal:if="true:z/txt" tal:replace="z/txt">source word</span
		>"<span tal:if="not:repeat/end" tal:omit-tag="">,</span>
		</span>
	      </span>

	    <br/>
	    <span class="note" tal:if="s/notes">
	      《<span tal:replace="s/notes">sense-notes</span>》<br/>
	      </span>

	    <span tal:repeat="g s/_gloss">
	      <span class="gloss" tal:replace="string:${g/gloss}. ">num.</span>
	      <span tal:if="not: eq: g/lang string:1" omit-tag="">
	        <span class="glang" tal:replace="kwabbr: 'LANG' g/lang">lang</span>:
		</span>
	      <span tal:if="not: eq: g/ginf string:1" omit-tag="">
	        [<span class="ginf" tal:replace="kwabbr: 'GINF' g/ginf">lang</span>.]
		</span>
	      <span class="gloss" tal:replace="g/txt">gloss</span><br/>
	      </span>

	    <table class="xrefs" tal:if="true:s/_xref">
	      <tr tal:if="true:s/_xref">
		<td colspan="2">Cross references:</td>   
	        </tr>
	      <tr tal:if="true:s/_xref" tal:repeat="x s/_xref">
		<td>&nbsp;</td>
		<td>
		  <span tal:replace="kwabbr: 'XREF' x/typ">xref-type</span>:
		  <a tal:attributes="href string:entr.pl?svc=${svc}&e=${x/xentr}" tal:content="x/ssum/seq">seq-num</a
		  ><span tal:if="gt: x/ssum/nsens string:1"
		    >(<span tal:replace="x/xsens">sense</span>) </span> 
		  <span tal:replace="x/ktxt">xref-kanj</span>
		  <span tal:if="and: x/ktxt x/rtxt" tal:omit-tag="">【</span>
		  <span tal:replace="x/rtxt">xref-rdng</span>
		  <span tal:if="and: x/ktxt x/rtxt" tal:omit-tag="">】</span>
		  <span tal:replace="x/ssum/gloss">gloss</span>
		  <span tal:if="x/notes" replace="x/notes">xref-note</span>
		  </td>
		</tr>
	      </table>
	    </td>
	  </tr>
        </table>

      <span tal:if="e/HAS_AUDIO" tal:omit-tag="">Audio:
	<dl>
	  <span tal:repeat="r e/_rdng" tal:omit-tag="">
	    <span tal:if="true:r/_audio" tal:omit-tag="">
	      <dt tal:content="r/txt">reading-text</dt>
	      <dd tal:repeat="a r/_audio">
		<a tal:attributes="href string:noaudio.html" tal:content="a/fname">filename</a>
		<span tal:replace="string:(${a/strt}:${a/leng})">(start:length)</span>
		</dd>
	      </span>
	    </span>
	  </dl>
	</span>

      <span tal:if="e/_hist" tal:omit-tag="">History:
        <table>
          <span tal:repeat="h e/_hist">
            <tr>
              <td>1.</td>
              <td class="hhdr" colspan="2" tal:content="string: ${kwabbr:'STAT' h/stat} ${h/dt} ${h/name}&lt;${h/email}&gt;">hist-header</td>
              </tr>
            <tr tal:if="h/notes">
              <td>&nbsp;</td>
              <td valign="top">Comments:</td>
              <td tal:content="h/notes">notes</td>
              </tr>
            <tr tal:if="h/refs">
              <td>&nbsp;</td>
              <td valign="top">Refs:</td>
              <td tal:content="h/refs">refs</td>
             </tr>
            </span>
          </table>
        </span>

      <hr/>
      <form action="edsubmit.pl" tal:attributes="method method">
	<input type="hidden" name="svc" tal:if="true:svc" tal:attributes="value svc"/>
	<input type="hidden" name="entr" value="${serialized}"/>
	<input type="Submit" value="Submit"/> 
	<span class="caution" tal:if="true:disp">
	  &nbsp;&nbsp;
	  <span tal:if="eq: disp string:a" tal:omit-tag="">Approve</span>
	  <span tal:if="eq: disp string:r" tal:omit-tag="">Reject</span>
	  <input type="hidden" name="disp" tal:attributes="value disp"/>
	  </span>
	</form>

      </div>
    </span>
  </body>
</html>
