<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <link rel="stylesheet" type="text/css" href="../jmdict.css"/>
  <title>JMdict Entry Edit Form</title>
</head>
<body class="edform">
<h1>JMdict Entry Edit Form</h1>
<form method="get" action="edconf.pl" name="edconf">
  <table class="form">
    <tbody>
      <tr>
        <td>
        <table class="top">
          <tbody>

	    <tr>
	      <td>Entry: <br/><a class="helplink" href="edhelp.pl">help</a></td>
	      <td>

		<!-- Entry block -->
		<table width="100%">
		  <tr tal:if="false:e">
		    <td colspan="4">New Entry</td></tr>
		  <tr tal:if="true:e">
		    <td>Id:</td>
		    <td tal:contents="e/id"></td>
		    <td>Stat:</td>
		    <td tal:contents="kwabbr: 'STAT' e/stat"></td>
		    </tr>
		  <tr>
		    <td>Corpus: <a class="helplink" href="edhelp.pl#corpus">help</a></td>
		    <td>
		      <select name="src">
			<span tal:repeat="s srcs" tal:omit-tag="">
			  <option name="src" tal:if="false:e" tal:attributes="value s/id" tal:contents="s/kw"/>
			  <span tal:if="true:e" tal:omit-tag="">
			    <option name="src" tal:if="eq: s/id e/src" selected="1" tal:attributes="value s/id" tal:contents="s/kw"/>
			    <option name="src" tal:if="not:eq: s/id e/src"          tal:attributes="value s/id" tal:contents="s/kw"/>
			    </span>
			  </span>
			</select>
		      </td>
		    <td>Seq: <a class="helplink" href="edhelp.pl#seq">help</a></td>
		    <td tal:if="false:e"><input type="text" name="seq" size="8" tal:attributes="value 'default'"/></td>
		    <td tal:if="true:e"><input type="text" name="seq" size="8" tal:attributes="value e/seq"/></td>
		    </tr>
		  <tr>
		    <td>Src note:</td><td colspan="3">
		      <input name="srcnote" tal:if="false:e" type="text" width="100%"/>
		      <input name="srcnote" tal:if="true:e" type="text" width="100%" tal:attributes="value e/srcnote"/>
		      </td>
		    </tr>
		  <tr>
		    <td>Entr note:</td><td colspan="3">
		      <textarea name="notes" tal:if="false:e" wrap="physical" width="100%" rows="3"></textarea>
		      <textarea name="notes" tal:if="true:e" wrap="physical" width="100%" rows="3" tal:contents="e/notes"></textarea>
		      </td>
		    </tr>
		  </table>

		</td>
	      </tr>
	    <tr>
	      <td>Kanji: <br/><a class="helplink" href="edhelp.pl#kanji">help</a></td>
	      <td><textarea tal:content="ktxt" name="kanj" wrap="physical" cols="60" rows="3"></textarea></td>
	      <td>
		<a class="helplink" href="edhelp.pl#kw_kinf">kinf</a><br/>
		<a class="helplink" href="edhelp.pl#kw_freq">freq</a> 
		</td></tr>
	    <tr>
	      <td>Readings: <br/><a class="helplink" href="edhelp.pl#readings">help</a></td>
	      <td><textarea tal:content="rtxt" name="rdng" wrap="physical" cols="60" rows="3"></textarea></td>
	      <td>
		<a class="helplink" href="edhelp.pl#kw_rinf">rinf</a><br/>
		<a class="helplink" href="edhelp.pl#kw_freq">freq</a><br/>
		<a class="helplink" href="edhelp.pl#syn_restr">restr</a>
		</td></tr>
	    <tr>
	      <td>Senses: <br/><a class="helplink" href="edhelp.pl#senses">help</a></td>
	      <td><textarea tal:content="stxt" name="sens" wrap="physical" cols="60" rows="8"></textarea></td>
	      <td>
		<a class="helplink" href="edhelp.pl#kw_pos">pos</a><br/>
		<a class="helplink" href="edhelp.pl#kw_misc">misc</a><br/>
		<a class="helplink" href="edhelp.pl#kw_fld">fld</a><br/>
		<a class="helplink" href="edhelp.pl#kw_dial">dial</a><br/>
		<a class="helplink" href="edhelp.pl#syn_lsrc">lsrc</a><br/>
		<a class="helplink" href="edhelp.pl#syn_lit">lit</a><br/>
		<a class="helplink" href="edhelp.pl#syn_expl">expl</a><br/>
		<a class="helplink" href="edhelp.pl#syn_restr">restr</a><br/>
		<a class="helplink" href="edhelp.pl#syn_xref">xref</a><br/>
		</td></tr>
	    <tr>
	      <td>References: <br/><a class="helplink" href="edhelp.pl#references">help</a></td>
	      <td><textarea name="reference" wrap="physical" cols="60" rows="3"></textarea></td>
	      <td></td>
	      </tr>
	    <tr>
	      <td>Comments: <br/><a class="helplink" href="edhelp.pl#comments">help</a></td>
	      <td><textarea name="comment" wrap="physical" cols="60" rows="3"></textarea></td>
	      <td></td>
	      </tr>
	    <tr>
	      <td>Name: <a class="helplink" href="edhelp.pl#name">help</a></td>
	      <td><input type="text" name="name" size="30"/></td>
	      <td></td>
	      </tr>
	    <tr>
	      <td>Email: <a class="helplink" href="edhelp.pl#email">help</a></td>
	      <td><input type="text" name="email" size="30"/></td>
	      <td></td>
	      </tr>
	    <tr>
	      <td></td>
	      <td>
		<input type="submit" value="Submit"/> &nbsp;

		<input type="hidden" name="id"  tal:if="e" tal:attributes="value e/id"/>
		<input type="hidden" name="seq" tal:if="e" tal:attributes="value e/seq"/>
		<input type="hidden" name="src" tal:if="e" tal:attributes="value e/src"/>
		<input type="hidden" name="stat" tal:if="e" tal:attributes="value e/stat"/>
		<input type="hidden" name="notes" tal:if="e" tal:attributes="value e/notes"/>
		<input type="hidden" name="srcnote" tal:if="e" tal:attributes="value e/srcnote"/>
		<input type="hidden" name="etxt" tal:attributes="value etxt"/>
		<input type="hidden" name="svc" tal:attributes="value svc"/>
		</td>
	      <td></td>
	      </tr>

            </tbody>
          </table>
          </td>
        </tr>
      </tbody>
    </table>
  </form>
<span class="notes">

  </span>
</body>
</html>
