<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <link rel="stylesheet" type="text/css" href="entr.css"/>
  <title>JMdict search form</title>
</head>
<body class="srchfrm">
<form method="get" action="srchres.pl" name="srchres">
  <table class="form">
    <tbody>
      <tr>
        <td>
        <table class="top">
          <tbody>
            <tr>
              <td>
              <table class="txtsrch">
                <tbody>
                  <tr>
                    <td>Search in</td>
                    <td>Search type</td>
                    <td>For text</td>
                  </tr>
                  <tr>
                    <td>
                    <select name="s1">
                    <option value="auto" selected="selected">Auto</option>
                    <option value="kanj">Kanji</option>
                    <option value="rdng">Reading</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                    <td>
                    <select name="y1">
                    <option selected="selected">Is</option>
                    <option>Starts</option>
                    <option>Contains</option>
                    <option>Ends</option>
                    </select>
                    </td>
                    <td><input name="t1"/></td>
                  </tr>
                  <tr>
                    <td>
                    <select name="s2">
                    <option value="auto" selected="selected">Auto</option>
                    <option value="kanj">Kanji</option>
                    <option value="rdng">Reading</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                    <td>
                    <select name="y2">
                    <option selected="selected">Is</option>
                    <option>Starts</option>
                    <option>Contains</option>
                    <option>Ends</option>
                    </select>
                    </td>
                    <td><input name="t2"/></td>
                  </tr>
                  <tr>
                    <td>
                    <select name="s3">
                    <option value="auto" selected="selected">Auto</option>
                    <option value="kanj">Kanji</option>
                    <option value="rdng">Reading</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                    <td>
                    <select name="y3">
                    <option selected="selected">Is</option>
                    <option>Starts</option>
                    <option>Contains</option>
                    <option>Ends</option>
                    </select>
                    </td>
                    <td><input name="t3"/></td>
                  </tr>
                </tbody>
              </table>
              </td>
              <td>Search by Id number:
              <table class="idsrch">
                <tbody>
                  <tr>
                    <td>Table:</td>
                    <td>
                    <select name="idtyp">
                    <option value="entr">Entry</option>
                    <option value="seqnum">Seq num</option>
                    <option value="rdng">Reading</option>
                    <option value="kanj">Kanji</option>
                    <option value="sens">Sense</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                  </tr>
                  <tr>
                  </tr>
                  <tr>
                    <td>Id number:</td>
                    <td><input name="idval" size="7"/></td>
                  </tr>
                </tbody>
              </table>
	      <br/><input name="search" value="Search" type="submit"/> 
              </td>
            </tr>
	    <tr><td colspan="2"> </td></tr>
            <tr>
              <td colspan="2"><span class="descriptor">Status:</span>
              <table class="optgroup">
                <tbody>
                  <tr tal:repeat="r stat">
                    <td tal:repeat="c r"> 
                      <input name="stat" tal:condition="c" tal:attributes="value c/id" tal:content="c/descr" type="checkbox"/> 
		    </td>
                  </tr>
                </tbody>
              </table>
              </td>
            </tr>
	    <tr><td colspan="2"><hr width="100%"/></td></tr>
            <tr>
              <td colspan="2"><span class="descriptor">Frequency of use:</span><br/>
              <span tal:repeat="z freq" class="optgroup">
                <input name="freq" tal:attributes="value z" tal:content="z" type="checkbox"/> 
	      </span> 
	      <br/><span class="optgroup">
	      <input name="freq" value="nf" type="checkbox"/>nf
              <select name="nfcmp">
              <option selected="selected">&lt;=</option>
              <option>=</option>
              <option>&gt;=</option>
              </select>
              <input name="nfval" size="2"/>

	      &nbsp;&nbsp;
	      <input name="freq" value="ga" type="checkbox"/>gA
              <select name="gacmp">
              <option selected="selected">&lt;=</option>
              <option>=</option>
              <option>&gt;=</option>
              </select>
              <input name="gaval" size="8"/></span>
	      </td>
            </tr>
	    <tr><td colspan="2"><hr width="100%"/></td></tr>
            <tr>
              <td colspan="2"><span class="descriptor">Part-of-speech attributes:</span>
              <table class="optgroup">
                <tbody>
                  <tr tal:repeat="r pos">
                    <td tal:repeat="c r"> 
                      <input name="pos" tal:condition="c" tal:attributes="value c/id" tal:content="c/kw" type="checkbox"/> 
		    </td>
                  </tr>
                </tbody>
              </table>
              </td>
            </tr>
	    <tr><td colspan="2"><hr width="100%"/></td></tr>
            <tr>
              <td colspan="2"><span class="descriptor">Misc attributes:</span>
              <table class="optgroup">
                <tbody>
                  <tr tal:repeat="r misc">
                    <td tal:repeat="c r"> 
                      <input name="misc" tal:condition="c" tal:attributes="value c/id" tal:content="c/kw" type="checkbox"/> 
		    </td>
                  </tr>
                </tbody>
              </table>
              </td>
            </tr>
          </tbody>
        </table>
        </td>
      </tr>
    </tbody>
  </table>
</form>
</body>
</html>
