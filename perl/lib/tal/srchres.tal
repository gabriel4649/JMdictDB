<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <link rel="stylesheet" type="text/css" href="entr.css"/>
  <title>JMdict search results</title>
</head>
<body class="srchres">
  <h1>JMdict Search Results</h1>
  <div class="results">
    <form method="get" action="entr.pl">
      <span class="count">
        <span tal:replace="len:results"/> matching entries found...
	</span>
      <input tal:if="gt:len:results 10" name="show" value="Display selected entries" type="submit" tal:condition="true:results"/> 
      <input tal:if="gt:len:results 10" type="reset" value="Reset" tal:condition="true:results"/> 
      <table class="reslist" tal:condition="true:results">
	<tr class="heading">
	  <th> </th>
	  <th>Seqnum</th>
	  <th>Reading</th>
	  <th>Kanji</th>
	  <th>Gloss</th>
	  </tr>
	<tr class="resrow" tal:repeat="r results">
	  <td><input type="checkbox" name="e" tal:attributes="value r/id"/></td>
	  <td class="seq">
	    <span tal:replace="kwabbr: 'SRC' r/src"/>
	    <span tal:replace="r/seq"/>
	    </td>
	  <td class="rdng" tal:content="r/rdng"/>
	  <td class="kanj" tal:content="r/kanj"/>
	  <td class="gloss" tal:content="r/gloss"/>
	  </tr>
	</table>
      <input name="show" value="Display selected entries" type="submit" tal:condition="true:results"/> 
      <input name="show" value="Reset" type="reset" tal:condition="true:results"/> 
      </form>
    </div>
  <div class="debug" tal:if="true:dbg">
    <span tal:repeat="d h2l: dbg">
      <span tal:replace="d/key">key</span>: <span tal:replace="d/val">value</span><br/>
      </span>
    </div>
  </body>
</html>