{%- extends "layout.jinja" %}
{% block title %}Search Results{% endblock %}
{%- block page_content %}
<!-- jbedits.jinja, part of JMdictDB.  Copyright (c) 2018 Stuart McGraw -->
    <div class="results">
      Wwwjdic preprocessed submission data in <a href="httpdir">{{filesdir}}</a>

      <br>Log files:
      <table>
      {% for f in logfiles %}
        <tr><td><a href="{{httpdir}}/{{f}}">{{f}}</a></td></tr>
        {% endfor %}
        </table>

      <br>Editdata files:
      <table>
      {% for f in editfiles %}
        <tr><td><a href="jbedit.py?svc={{svc}}&amp;sid={{sid|d}}&amp;fn={{f}}">{{f}}</a></td></tr>
        {% endfor %}
        </table>

      </div>
  {% endblock %}
