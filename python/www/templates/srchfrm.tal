<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<!-- Copyright (C) 2006, Stuart McGraw -->
<html>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  <link rel="stylesheet" type="text/css" href="entr.css">
  <title>JMdict</title>
</head>
<body>
<form method="get" action="srchres.py" name="srchres">
  <table border="1">
    <tbody>
      <tr>
        <td>
        <table border="1">
          <tbody>
            <tr>
              <td>
              <table border="1">
                <tbody>
                  <tr>
                    <td>Search in</td>
                    <td>Search type</td>
                    <td>For text</td>
                  </tr>
                  <tr>
                    <td>
                    <select name="s1">
                    <option value="auto" selected="selected">Auto</option>
                    <option value="kanj">Kanji</option>
                    <option value="kana">Reading</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                    <td>
                    <select name="y1">
                    <option selected="selected">Is</option>
                    <option>Starts</option>
                    <option>Contains</option>
                    <option>Ends</option>
                    </select>
                    </td>
                    <td><input name="t1"></td>
                  </tr>
                  <tr>
                    <td>
                    <select name="s2">
                    <option value="auto" selected="selected">Auto</option>
                    <option value="kanj">Kanji</option>
                    <option value="kana">Reading</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                    <td>
                    <select name="y2">
                    <option selected="selected">Is</option>
                    <option>Starts</option>
                    <option>Contains</option>
                    <option>Ends</option>
                    </select>
                    </td>
                    <td><input name="t2"></td>
                  </tr>
                  <tr>
                    <td>
                    <select name="s3">
                    <option value="auto" selected="selected">Auto</option>
                    <option value="kanj">Kanji</option>
                    <option value="kana">Reading</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                    <td>
                    <select name="y3">
                    <option selected="selected">Is</option>
                    <option>Starts</option>
                    <option>Contains</option>
                    <option>Ends</option>
                    </select>
                    </td>
                    <td><input name="t3"></td>
                  </tr>
                </tbody>
              </table>
              </td>
              <td> <input name="search" value="Search" type="submit"> 
	      <br>Search by Id number:
              <table>
                <tbody>
                  <tr>
                    <td>Table:</td>
                    <td>
                    <select name="idtyp">
                    <option value="entr">Entry</option>
                    <option value="seqnum">Seq num</option>
                    <option value="kana">Reading</option>
                    <option value="kanj">Kanji</option>
                    <option value="sens">Sense</option>
                    <option value="gloss">Gloss</option>
                    </select>
                    </td>
                  </tr>
                  <tr>
                  </tr>
                  <tr>
                    <td>Id number:</td>
                    <td><input name="idval" size="7"></td>
                  </tr>
                </tbody>
              </table>
              </td>
            </tr>
            <tr>
              <td colspan="2"> Entry source:<br>
              <span tal:repeat="z src"> 
		<input name="src" tal:attributes="value z/id" tal:content="z/kw" type="checkbox"> 
		</span>
	      </td>
            </tr>
            <tr>
              <td colspan="2"> Frequency of use:<br>
              <span tal:repeat="z freq" tal:omit-tag="">
                <input name="freq" tal:attributes="value z" tal:content="z" type="checkbox"> 
	      </span> 
	      <br> 
	      <input name="freq" value="nf" type="checkbox">nf
              <select name="y3">
              <option selected="selected">&lt;</option>
              <option>=</option>
              <option>&gt;</option>
              </select>
              <input name="nfval" size="2"> </td>
            </tr>
            <tr>
              <td colspan="2"> Part-of-speech attributes:
              <table>
                <tbody>
                  <tr tal:repeat="r python:range(7)">
                    <td tal:repeat="c python:range(8)"> 
		      <span tal:define="z python:pos[r*8+c]" tal:condition="python: r*8+c&lt;len(pos)" tal:omit-tag="">
                        <input name="pos" tal:attributes="value z/id" tal:content="z/kw" type="checkbox"> 
		      </span> 
		    </td>
                  </tr>
                </tbody>
              </table>
              </td>
            </tr>
            <tr>
              <td colspan="2"> Misc attributes:
              <table>
                <tbody>
                  <tr tal:repeat="r python:range(4)">
                    <td tal:repeat="c python:range(8)"> 
		      <span tal:define="z python:misc[r*8+c]" tal:condition="python: r*8+c&lt;len(misc)" tal:omit-tag="">
                        <input name="pos" tal:attributes="value z/id" tal:content="z/kw" type="checkbox"> 
		      </span> 
		    </td>
                  </tr>
                </tbody>
              </table>
              </td>
            </tr>
          </tbody>
        </table>
        </td>
      </tr>
    </tbody>
  </table>
</form>
</body>
</html>
